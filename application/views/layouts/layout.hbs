<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="../public/css/test.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script defer src="./public/js/frontendjs.js"></script>
</head>
  <body>
    
    {{>navbar}}
    {{#if (emptyObject messages)}}
    {{>flashmessage}}
    {{/if}}
    {{{body}}}
    {{>footer}}

    {{#if logged}}
      <script>
        let ele = document.getElementById("logout-button");
        if(ele){
          ele.onclick = (even) =>{
            fetch("/users/logout", {
              method: "POST"
            })
            .then((data) => {
              location.replace("/");
            })
          }
        }
      </script>
    {{/if}}
  </body>
</html>
