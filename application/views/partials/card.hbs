<div id="post-{{id}}" class="card">
    <img class="card-image" src={{thumbnail}} alt="Missing Image">
    <div class="card-body">
        <p class="card-title">{{title}}</p>
        <p class="card-text">{{description}}</p>
    </div>
    <a id="card-post-det" class="card-details" href="/post/{{id}}" class="anchor-buttons">Post Details</a>
</div>